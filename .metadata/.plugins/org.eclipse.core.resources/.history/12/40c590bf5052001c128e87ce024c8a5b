package first_java;

public class Main {
	 public static String name;
	 
    void getCash() {
		int num= 10;
		num=50;
		num =100;
		name ="sabbir";
		System.out.println(num+name);
	}
	 

	public static void main(String[] args) {
		name= "Mizan";
		System.out.println(name);
		new Main().getCash();
	}
}
